  "typescript": {
    "main": "index.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ajnasz/hubot-pushbot"
  },
  "keywords": [
    "hubot",
    "hubot-scripts",
    "pushbot",
    "irc",
    "slack",
    "hipchat",
    "campfire"
  ],
  "author": {
    "name": "Lajos Koszti"
  },
  "license": "MIT",
  "dependencies": {
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-typescript": "^0.6.2",
    "typescript": "^1.5.0-beta",
    "xregexp": "^2.0.0"
  },
  "maintainers": [
    {
      "name": "Ajnasz",
      "email": "ajnasz@ajnasz.hu"
    }
